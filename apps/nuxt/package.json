{
  "name": "nuxt",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "dev:prepare": "nuxt prepare",
    "build": "nuxt build",
    "start": "nuxt start",
    "clean": "rm -rf .nuxt .output node_modules/.vite && pnpm store prune",
    "lint": "eslint .",
    "lint:types": "npx nuxi typecheck",
    "format": "eslint . --fix",
    "test": "pnpm dev:prepare && vitest run",
    "test:silent": "pnpm dev:prepare && vitest run --silent",
    "test:ui": "pnpm dev:prepare && vitest --ui",
    "test:coverage": "pnpm dev:prepare && vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@vueuse/core": "^13.9.0",
    "bullmq": "^4.0.0",
    "nuxt": "^3.13.0",
    "nuxt-auth-utils": "^0.5.25",
    "shared": "workspace:*",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/eslint-plugin": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^20.0.0",
    "@types/vue-router": "^2.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "supertest": "^7.1.4",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "vue-router": "^4.3.0",
    "vue-tsc": "^3.0.8",
    "vuetify-nuxt-module": "^0.18.7"
  }
}
