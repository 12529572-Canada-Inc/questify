{
  "name": "nuxt",
  "version": "1.7.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "dev:prepare": "nuxt prepare",
    "build": "nuxt build",
    "start": "nuxt start",
    "clean": "rm -rf .nuxt .output node_modules/.vite && pnpm store prune",
    "lint": "eslint .",
    "lint:types": "npx nuxi typecheck",
    "format": "eslint . --fix",
    "test:unit": "vitest run",
    "test:coverage": "pnpm dev:prepare && vitest run --coverage",
    "test:e2e": "NODE_ENV=test nuxt test --runner=playwright --config ./playwright.config.ts",
    "test": "pnpm dev:prepare && pnpm test:unit"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@vueuse/core": "^13.9.0",
    "@mdi/font": "^7.4.47",
    "markdown-it": "^14.1.0",
    "bullmq": "^4.0.0",
    "nuxi": "^3.29.3",
    "nuxt": "^4.1.3",
    "nuxt-auth-utils": "^0.5.25",
    "shared": "workspace:*",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/eslint-plugin": "^1.9.0",
    "@nuxt/kit": "^4.1.3",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/types": "^2.18.1",
    "@playwright/test": "^1.43.1",
    "@types/node": "^20.19.17",
    "@types/supertest": "^6.0.3",
    "@types/vue-router": "^2.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "listhen": "^1.9.0",
    "nitropack": "^2.12.7",
    "oxc-parser": "0.94.0",
    "supertest": "^7.1.4",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^3.0.8",
    "vuetify-nuxt-module": "^0.18.7"
  }
}
