{
  "name": "nuxt",
  "version": "1.16.0",
  "private": true,
  "type": "module",
  "imports": {
    "#imports": "./.nuxt/imports.mjs"
  },
  "scripts": {
    "dev": "nuxt dev",
    "dev:prepare": "nuxt prepare",
    "build": "nuxt build",
    "start": "nuxt start",
    "clean": "rm -rf .nuxt .output node_modules/.vite && pnpm store prune",
    "lint": "eslint .",
    "lint:types": "npx nuxi typecheck",
    "format": "eslint . --fix",
    "test:nuxt": "vitest run --config vitest.config.mts",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:coverage": "pnpm dev:prepare && vitest run --config vitest.unit.config.ts --coverage",
    "test:e2e": "playwright test",
    "test": "pnpm dev:prepare && pnpm test:nuxt && pnpm test:unit"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "6.17.1",
    "@vueuse/core": "^13.9.0",
    "bullmq": "^4.0.0",
    "markdown-it": "^14.1.0",
    "nuxi": "^3.29.3",
    "nuxt": "^4.1.3",
    "nuxt-auth-utils": "^0.5.25",
    "openai": "^4.0.0",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "shared": "workspace:*",
    "vue": "^3.5.21",
    "vuetify": "^3.10.6"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/eslint-plugin": "^1.9.0",
    "@nuxt/kit": "^4.2.0",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxt/types": "^2.18.1",
    "@playwright/test": "^1.43.1",
    "@types/node": "^20.19.17",
    "@types/supertest": "^6.0.3",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^4.0.14",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "listhen": "^1.9.0",
    "nitropack": "^2.12.7",
    "oxc-parser": "0.94.0",
    "supertest": "^7.1.4",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^3.1.3",
    "vuetify-nuxt-module": "^0.18.7"
  }
}
