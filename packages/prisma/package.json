{
  "name": "prisma",
  "version": "1.18.2",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "deploy": "prisma migrate deploy",
    "deploy:test": "dotenv -e .env.test -- prisma migrate deploy",
    "prepare": "prisma generate",
    "seed": "ts-node -r tsconfig-paths/register seed.ts",
    "seed:generate": "ts-node utils/generateSeedData.ts",
    "clear": "prisma migrate reset --force",
    "reset": "pnpm clear && pnpm seed"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.0.0",
    "@prisma/client": "7.0.0",
    "dotenv": "^16.4.5",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^11.0.0",
    "prisma": "7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  }
}
