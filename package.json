{
  "name": "questify",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:nuxt": "pnpm --filter nuxt dev",
    "dev:worker": "ts-node apps/worker/src/index.ts",
    "prisma:generate": "pnpm --filter prisma exec prisma generate",
    "db:migrate": "pnpm --filter prisma exec prisma migrate dev"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1"
  }
}
