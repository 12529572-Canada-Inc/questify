{
  "name": "questify",
  "private": true,
  "scripts": {
    "dev:nuxt": "pnpm --filter nuxt dev",
    "dev:worker": "pnpm --filter worker dev",
    "prisma:generate": "pnpm --filter prisma generate",
    "prisma:migrate": "pnpm --filter prisma migrate",
    "prisma:deploy": "pnpm --filter prisma deploy",
    "prisma:seed": "pnpm --filter prisma seed",
    "prisma:reset": "pnpm --filter prisma reset",
    "lint": "pnpm lint:nuxt && pnpm lint:worker",
    "lint:nuxt": "pnpm --filter nuxt lint",
    "lint:worker": "pnpm --filter worker lint",
    "format": "pnpm format:nuxt && pnpm format:worker",
    "format:nuxt": "pnpm --filter nuxt format",
    "format:worker": "pnpm --filter worker format",
    "test": "pnpm test:nuxt && pnpm test:worker",
    "test:nuxt": "pnpm --filter nuxt test",
    "test:worker": "pnpm --filter worker test",
    "prepare": "pnpm --filter prisma prepare",
    "build": "pnpm --parallel build"
  }
}
